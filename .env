# Application Environment
APP_ENV=development

# Server Configuration
SERVER_PORT=8080
SERVER_HOST=0.0.0.0
SERVER_READ_TIMEOUT=15s
SERVER_WRITE_TIMEOUT=15s

# Database Configuration
DB_HOST=postgres
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=postgres
DB_NAME=smartdriver
DB_SSLMODE=disable

# Centrifugo Configuration
CENTRIFUGO_PORT=8000
CENTRIFUGO_URL=http://centrifugo:8000
CENTRIFUGO_API_KEY=your-api-key-here
CENTRIFUGO_TOKEN_SECRET=your-secret-key-here
CENTRIFUGO_ADMIN_PASSWORD=admin
CENTRIFUGO_ADMIN_SECRET=admin-secret

# Parser Configuration
PARSER_POLLING_INTERVAL=30s
PARSER_BATCH_SIZE=100
PARSER_WORKER_COUNT=5